package book_practice;

import java.util.Scanner;

public class Ex10 {

	static Scanner scan = new Scanner(System.in);
	public static void main(String[] args) {
		Person p = new Person();
		System.out.print("1번째 선수 이름 >> ");
		p.setName(scan.next());
		System.out.print("2번째 선수 이름 >> ");
		p.setName(scan.next());
		
		System.out.println(p);
		
	}
}
