package view;

import model.DTO;

public class Search implements View {

	@Override
	public void display() {
		System.out.println();
		System.out.println(searchTitle);
		
		System.out.print("검색 할 학생 이름 입력 : ");
		data.dto.setName(scan.next());


	}

	public void select(DTO[] dtoArr) {
		
		System.out.println("----- 검색 결과 -----");
		
		for(DTO dto : dtoArr) {
			System.out.println(dto);
		}
		
	}

}
