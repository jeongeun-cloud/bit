package controller;

import static resource.R.*;

public class Controller {

	public static void service() {
		
		switch(data.no) {
		case 1 : 
			viewArr[data.no].display();
			break;
		case 2 : break;
		case 3 : break;
		case 4 : break;
		case 5 : break;
		case 6 : 
			System.exit(0);
			return;
		}
	}
	
}
