package model;

public class DAO {
	
	private static final int MAX = 100;
	private DTO[] dtoArr = new DTO[MAX];
	private int top = 0;
	private int seq = 1;
	

	// ª¿‘
	public void insert(DTO dto) {
		dto.setIdx(seq++);
		dtoArr[top++] = dto;
	}
	
	// ΩÃ±€≈Ê
	private DAO() {
		// TODO Auto-generated constructor stub
	}
	private static DAO instance;
	public static DAO getInstance() {
		if(instance == null) {
			instance = new DAO();
		}
		return instance;
	} 
	
	
	
}
