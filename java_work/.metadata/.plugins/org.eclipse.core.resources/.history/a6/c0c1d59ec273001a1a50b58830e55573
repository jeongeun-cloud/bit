package org.comstudy21.thread;

public class Ch06Ex03 implements Runnable {
	
	@Override
	public void run() {
		for(int i=0; i<50; i++) {
			System.out.println("Thread ½ÇÇà" + i);
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
		
	}

	public static void main(String[] args) {
		Thread t = new Thread(new Ch06Ex03());
		t.start();

	}


}
