package model;



public class DAO {
	public static final int MAX = 100;
	private DTO[] dtoArr = new DTO[MAX];
	private int top = 0;
	private int seq = 1;
	
	
	// ΩÃ±€≈Ê
	private DAO() {}
	private static DAO instance; 
	public static DAO getInstance() {
		if(instance == null) {
			instance = new DAO();
		}
		return instance;
	}
	

	public void insert(DTO dto) {
		dto.setIdx(seq++);
		dtoArr[top++] = dto;
	}

}
