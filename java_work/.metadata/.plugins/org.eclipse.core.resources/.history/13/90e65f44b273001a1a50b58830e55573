package book_practice;

class Point {
	private int x, y;
	public Point() {
		// TODO Auto-generated constructor stub
	}
	
	public Point (int x, int y) {
		this.x = x; 
		this.y = y;
	}
	
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	public void setX(int x) {
		this.x = x;
	}
	public void setY(int y) {
		this.y = y;
	}
	
	protected void move (int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	
}

class ColorPrint extends Point {
	String color = "";
	
	public ColorPrint(int x, int y, String color) {
//		super.setX(x);
//		super.setY(y);
//		this.color = color;
		
	}

	public void setXY(int x, int y) {
		super.setX(x);
		super.setY(y);
	}

	public void setColor(String color) {
		this.color = color;
		
	}

	public String getColor() {
		return color;
	}

	@Override
	public String toString() {
		return "ColorPrint [color=" + color + "]";
	}
	
	
	
	
}

public class Page317 {
	public static void main(String[] args) {
		ColorPrint cp = new ColorPrint(5, 5, "YELLOW");
		cp.setXY(10,20);
		cp.setColor("RED");
		String str = cp.toString();
		System.out.println(str + "¿‘¥œ¥Ÿ.");
	}
}
