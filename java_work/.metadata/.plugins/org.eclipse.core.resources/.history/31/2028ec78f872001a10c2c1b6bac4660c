package book_practice;

class Point {
	private int x, y;
	public Point (int x, int y) {
		this.x = x; 
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public int getY() {
		return y;
	}
	protected void move (int x, int y) {
		this.x = x;
		this.y = y;
	}
	
}

class ColorPrint {
	public ColorPrint(int x, int y, String color) {

	}
	
	public void setXY(int x, int y) {
		
	}
	
	public void setColor(String color) {
		
	}

	@Override
	public String toString() {
		return "ColorPrint [toString()=" + super.toString() + "]";
	}
	
	
}

public class Page317 {
	public static void main(String[] args) {
		ColorPrint cp = new ColorPrint(5, 5, "YELLOW");
		cp.setXY(10,20);
		cp.setColor("RED");
		String str = cp.toString();
		System.out.println(str + "¿‘¥œ¥Ÿ.");
	}
}
