package controller;

import static resource.R.*;

public class Controller {
	
	public static void service() {
		
		switch(data.no) {
		case 1 : break;
		case 2 : break;
		case 3 : break;
		case 4 : break;
		case 5 : break;
		case 6 : 
			System.out.println("시스템을 종료 합니다!");
			System.exit(0);
			break;
		default : 
			System.out.println("번호를 잘 못 입력 하였습니다!");
			break;
		}
	}

}
